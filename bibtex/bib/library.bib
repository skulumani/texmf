  File                     = {koon2004.pdf:koon2004.pdf:PDF},
  ISSN                     = {1749-6632},
  Keywords                 = {geometric mechanics, dynamics, asteroid pairs, full-body problem},
  Publisher                = {Blackwell Publishing Ltd},
  Url                      = {http://dx.doi.org/10.1196/annals.1311.002}
}

@Article{demars2012,
  Title                    = {Initial Orbit Determination using Short-Arc Angle and Angle Rate Data},
  Author                   = {K. J. DeMars and M. K. Jah and P. W. Schumacher},
