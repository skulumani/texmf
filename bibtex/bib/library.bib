@Article{yu2007,
  author        = {Yu, Hui and Baoyin, He-Xi and Li, Jun-Feng},
  title         = {Low energy transfer to the moon using the patching of invariant manifolds of two there-body systems.},
  journal       = {Yuhang Xuebao / Journal of Astronautics},
  year          = {2007},
  volume        = {28},
  number        = {3},
  pages         = {637--642},
  abstract      = {Traditional method is Hohmonn transfer following the two-body model, but the Japanese Hiten mission succeeded in exploring the Moon with lower energy in 1991 utilizing perturbation of the Sun. In this paper, using the invariant manifolds of nonlinear 3-body systems, we constructed low energy transfer trajectories to the Moon. Along the method used by JPL research group, we approximated the Sun-Earth-Moon-spacecraft four-body system as two plane circular restricted there-body systems, and then presented more profound explanation for Hiten s trajectory in mathematics and dynamics. Considering the direction of the rotation of Earth-Moon L2 stable manifolds, we presented their structures and a more reasonable patching method, and obtained the position and velocity of the spacecraft at the launch point, and finally designed a trajectory which requires 12{\%} less velocity increment than traditional method. The results prove the feasibility and advantage of invariant manifolds of 3-body systems in designing and constructing trajectories from the Earth to the Moon.},
  address       = {School of Aerospace, Tsinghua University, Beijing 100084, China yuhui05{\char64}mails.thu.edu.cn},
  an            = {30065583; 200710-12-485646 (MT); A07-12-070249 (AH)},
