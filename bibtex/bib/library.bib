  title         = {Constrained spacecraft attitude control on ${\SO}$ using fast nonlinear model predictive control using reference governors and nonlinear model predictive control},
  booktitle     = {American Control Conference (ACC), 2014},
  year          = {2014},
  pages         = {5586--5593},
