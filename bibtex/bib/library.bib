@InProceedings{lee2007b,
  author     = {Taeyoung Lee and N. A. Chaturvedi and A. Sanya and M. Leok and N. H. McClamroch},
  title      = {{Propagation of uncertainty in rigid body attitude flows}},
  booktitle  = {Proceedings of 46\textsuperscript{th} IEEE Conference on Decision and Control},
  year       = {2007},
  pages      = {2689--2694},
  month      = dec,
  abstract   = {Motivated by attitude control and attitude estimation problems for a rigid body, computational methods are proposed to propagate uncertainties in the angular velocity and the attitude. Uncertainties in the angular velocity and attitude are described in terms of ellipsoidal sets that are propagated through this nonlinear attitude flow. Computational methods are proposed, one method based on a local linearization of the attitude flow and two methods based on propagation of a small (unscented) sample selected from the initial uncertainty ellipsoid. Each of these computational methods is constructed using a Lie group variational integrator, viewed as a discretization of the attitude dynamics. Computational results are obtained that indicate: (1) the strongly nonlinear attitude flow characteristics; and (2) the limitations of each of these methods, and indeed any method, in providing effective global bounds on the nonlinear attitude flow.},
  doi        = {10.1109/CDC.2007.4434145},
  eprint     = {math/0702737},
  eprinttype = {arxiv},
  file       = {IEEE paper:lee2007b.pdf:PDF;arxiv paper:lee2007b_2.pdf:PDF},
  groups     = {Islam paper},
  keywords   = {Lie groups,angular velocity control,attitude control,flow control,nonlinear control systems,pendulums,shear modulus,uncertain systems,3D pendulum dynamics,Lie group variational integrator,angular velocity,attitude control,attitude dynamics discretization,attitude estimation problems,computational methods,ellipsoidal sets,local linearization,nonlinear attitude flow,rigid body attitude flows,uncertainty propagation,Aerodynamics,Angular velocity,Angular velocity control,Attitude control,Ellipsoids,Gravity,Nonlinear dynamical systems,Partial differential equations,Position measurement,Uncertainty},
  owner      = {shankar},
  timestamp  = {2017.02.14 12:02},
}

