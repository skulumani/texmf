@Article{dickey1994,
  author     = {Dickey, J. O. and Bender, P. L. and Faller, J. E. and Newhall, X X and Ricklefs, R. L. and Ries, J. G. and Shelus, P. J. and Veillet, C. and Whipple, A. L. and Wiant, J. R. and Williams, J. G. and Yoder, C. F.},
  title      = {Lunar Laser Ranging: A Continuing Legacy of the Apollo Program},
  journal    = {Science},
  year       = {1994},
  volume     = {265},
  number     = {5171},
  pages      = {482--490},
  issn       = {0036-8075},
  abstract   = {On 21 July 1969, during the first manned lunar mission, Apollo 11, the first retroreflector array was placed on the moon, enabling highly accurate measurements of the Earthmoon separation by means of laser ranging. Lunar laser ranging (LLR) turns the Earthmoon system into a laboratory for a broad range of investigations, including astronomy, lunar science, gravitational physics, geodesy, and geodynamics. Contributions from LLR include the three-orders-of-magnitude improvement in accuracy in the lunar ephemeris, a several-orders-of-magnitude improvement in the measurement of the variations in the moon{\textquoteright}s rotation, and the verification of the principle of equivalence for massive bodies with unprecedented accuracy. Lunar laser ranging analysis has provided measurements of the Earth{\textquoteright}s precession, the moon{\textquoteright}s tidal acceleration, and lunar rotational dissipation. These scientific results, current technological developments, and prospects for the future are discussed here.},
  comment    = {* lunar rangng has provided lots of good results (geodessy, earth orientation, gravity etc)
* 10^19 photos are sent out per seconds and a signal loss of 10^-21 requires single photon detection capability
* Moon orbit is 2-4 orders of magnitude better now with lunar ranging
* Used to accurately determine the obliquity of the ecliptic
* Lots of useful data},
  doi        = {10.1126/science.265.5171.482},
  eprint     = {http://science.sciencemag.org/content/265/5171/482.full.pdf},
  file       = {:dickey1994.pdf:PDF},
  groups     = {Laser asteroid shape},
  keywords   = {lidar, laser, read},
  owner      = {shankar},
  publisher  = {American Association for the Advancement of Science},
  timestamp  = {2018.07.06 00:07},
  url        = {http://science.sciencemag.org/content/265/5171/482},
}

@Article{amann2001,
  author     = {Markus-Christian Amann and Thierry M. Bosch and Marc Lescure and Risto A. Myllylae and Marc Rioux},
  title      = {Laser ranging: a critical review of unusual techniques for distance measurement},
