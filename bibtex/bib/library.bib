  bdsk-url-2    = {http://dx.doi.org/10.1016/j.actaastro.2013.03.013},
  date-added    = {2014-12-22 15:18:00 +0000},
  date-modified = {2014-12-22 15:18:04 +0000},
  doi           = {http://dx.doi.org/10.1016/j.actaastro.2013.03.013},
  file          = {finocchietti2014.pdf:finocchietti2014.pdf:PDF},
  keywords      = {Poincar{\`e} section},
