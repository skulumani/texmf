@Article{mingotti2011,
  author        = {Mingotti, G. and Topputo, F. and Bernelli-Zazzera, F.},
  title         = {Earth--mars Transfers with Ballistic Escape and Low-thrust Capture},
  journal       = {Celestial Mechanics and Dynamical Astronomy},
  year          = {2011},
  volume        = {110},
  number        = {2},
  pages         = {169--188},
  issn          = {0923-2958},
  abstract      = {In this paper novel Earth--Mars transfers are presented. These transfers exploit the natural dynamics of n-body models as well as the high specific impulse typical of low- thrust systems. The Moon-perturbed version of the Sun--Earth problem is introduced to design ballistic escape orbits performing lunar gravity assists. The ballistic capture is designed in the Sun--Mars system where special attainable sets are defined and used to handle the low- thrust control. The complete trajectory is optimized in the full n-body problem which takes into account planets' orbital inclinations and eccentricities. Accurate, efficient solutions with reasonable flight times are presented and compared with known results.},
  annote        = {use of low-thrust transfers rather than impulsive manuevers define attainable sets rather than the intersection of invariant manifolds lots of good reference about low thrust and stable manifolds earth escape velocity is less than the standard hyperbolic escape. referred to as ballistic escape by exploiting the sun/moon gravity fields uses a restricted four body problem by treating the moon as a perturbing force in the three body system but equilibrium points, periodic orbits, and invariant manifolds disappear . loss of geometrical properties sets defined that govern intersection of transfer/parking orbits a thrust law is given in equation 12. increase velocity in heliocentric orbit then decrease energy when in the sun/mars regime transfer is assumed complete when spacecraft reaches periapsis around mars. may not be in a mars orbit? spacecraft trajectory is optimized within a full 4-body dynamical model of the system optimization routine broken up into stages(earth depart, heliocentric, mars arrival) to avoid large distance changes whole transfer trajectory is discretized time intervals are chosen by hand and vary by regime (short near earth/mars and longer in the heliocentric phase)},
  bdsk-file-1   = {},
  bdsk-url-1    = {http://dx.doi.org/10.1007/s10569-011-9343-5},
  date-added    = {2014-08-16 12:21:03 +0000},
  date-modified = {2014-08-19 12:39:24 +0000},
  doi           = {10.1007/s10569-011-9343-5},
  file          = {mingotti2011.pdf:mingotti2011.pdf:PDF},
  keywords      = {Restricted three-body problem; Invariant manifolds; Low-thrust transfer; n-body models; Ballistic capture; Lunar-gravity assist},
  language      = {English},
  publisher     = {Springer Netherlands},
  read          = {1},
  url           = {http://dx.doi.org/10.1007/s10569-011-9343-5},
}

@Article{ozimek2010a,
  author        = {Ozimek, M. T. and Howell, K. C.},
  title         = {Low-thrust Transfers in the Earth-moon System, Including Applications to Libration Point Orbits},
  journal       = {Journal of Guidance, Control, and Dynamics},
  year          = {2010},
  volume        = {33},
  number        = {2},
  pages         = {533--549},
  month         = {2014/08/16},
  abstract      = {Preliminary designs of low-thrust transfer trajectories are developed in the Earth--moon three-body problem with variable specific impulse engines and fixed engine power. The solution for a complete time history of the thrust magnitude and direction is initially approached as a calculus of variations problem to locally maximize the final spacecraft mass. The problem is then solved directly by sequential quadratic programming, using either single or multiple shooting. The coasting phase along the transfer exploits invariant manifolds and, when possible, considers locations along the entire manifold surface for insertion. Such an approach allows for a nearly propellant-free final coasting phase along an arc selected from a family of known trajectories that contract to the periodic libration point orbit. This investigation includes transfer trajectories from an Earth parking orbit to some sample libration point trajectories, including L1 halo orbits, L1 and L2 vertical orbits, and L2 butterfly orbits. Given the availability of variable specific impulse engines in the future, this study indicates that fuel-efficient transfer trajectories could be used in future lunar missions, such as south pole communications satellite architectures.},
  annote        = {doi: 10.2514/1.43179 use the stable/unstalbe manifolds to determine useful low energy transfer orbits use stable/unstable eigenvectors at an equilibrium point to define initial state direction perturbation to propogate the manifold trajectories calculus of variations approach by applying all constraints via lagrange multipliers continuous low thrust dynamics investigated},
