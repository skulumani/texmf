@Article{braun2007,
  author     = {Braun, Robert D. and Manning, Robert M.},
  title      = {Mars Exploration Entry, Descent, and Landing Challenges},
  journal    = {Journal of Spacecraft and Rockets},
  year       = {2007},
  volume     = {44},
  comment    = {* Summary of past successful landing systems and approaches
* Human landing schemes for Mars for bigger loads and higher elevations
* Mars entry, descent and landing is difficult:
    1. Thick enough atmosphere for heating but not enough for low terminal descent velocity
        * Must have balistic coefficient  50 kg/m^3 to deliver payload to subsonic conditions
        * Hypersonic deceleration happens at lower altitude so there is not much time remaining for landing
        * All US landing missions still use a Viking era 70 deg sphere cone for high hypersonic drag coefficient
    2. Complex surface with highly irregular terrain
        * Mars has a bi-modal surface altitude distribution. 
        * Half of Mars has currently been out of reach due to high elevation
        * Lots of large rocks and difficult to see from orbit make legged landers susceptible
        * Landing site slope greater than 3m is difficult to measure from orbit
    3. Difficult to accurately recreate Mars environment for EDL testing and qualification
        * Short time for EDL and switching between computer systems mean there is frequently no redundancy
        * Not able to test on Earth easily so will use older systems which have flown

* Sensors are expensive: Radar is cheaper than Doppler for measuring horizontal velocity
* All US Mars landers still use the Viking developed aeroshell, thermal protective material, supersonic parachute},
  doi        = {doi: 10.2514/1.25116},
  file       = {:braun2007.pdf:PDF},
  groups     = {Surface Landing},
  keywords   = {landing, astrodynamics, spacecraft, skimmed},
  owner      = {shankar},
  publisher  = {American Institute of Aeronautics and Astronautics},
  timestamp  = {2018.07.06 00:07},
  url        = {http://dx.doi.org/10.2514/1.25116},
}

