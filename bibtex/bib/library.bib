@Article{chanut2014,
  author        = {Chanut, T. G. G. and Winter, O. C. and Tsuchida, M.},
  title         = {{3D} Stability Orbits Close to 433 Eros Using an Effective Polyhedral Model Method},
  journal       = {Monthly Notices of the Royal Astronomical Society},
  year          = {2014},
  abstract      = {One of the techniques used in the past decade to determine the shape with a good accuracy and estimate certain physical features (volume, mass, moments of inertia) of asteroids is the polyhedral model method. We rebuild the shape of the asteroid 433 Eros using data from 1998 December observations of the probe Near-Earth-Asteroid-Rendezvous-Shoemaker. In our computations, we use a code that avoids singularities from the line integrals of a homogeneous arbitrary shaped polyhedral source. This code evaluates the gravitational potential function and its first- and second-order derivatives. Taking the rotation of asteroid 433 Eros into consideration, the aim of this work is to analyse the dynamics of numerical simulations of 3D initially equatorial orbits near the body. We find that the minimum radius for direct, equatorial circular orbits that cannot impact with the Eros surface is 36\hspace{0.167em}km and the minimum radius for stable orbits is 31\hspace{0.167em}km despite significant perturbations of its orbit. Moreover, as the orbits suffer less perturbations due to the irregular gravitational potential of Eros in the elliptic case, the most significant result of the analysis is that stable orbits exist at a periapsis radius of 29\hspace{0.167em}km for initial eccentricities ei $\leq$ 0.2.},
  bdsk-file-1   = {},
  bdsk-url-1    = {http://mnras.oxfordjournals.org/content/early/2014/01/09/mnras.stt2383.abstract},
  bdsk-url-2    = {http://dx.doi.org/10.1093/mnras/stt2383},
  date-added    = {2016-08-29 22:19:54 +0000},
  date-modified = {2016-08-29 22:19:55 +0000},
  doi           = {10.1093/mnras/stt2383},
  eprint        = {http://mnras.oxfordjournals.org/content/early/2014/01/09/mnras.stt2383.full.pdf+html},
  file          = {chanut2014.pdf:chanut2014.pdf:PDF},
  timestamp     = {2018.07.06 00:07},
  url           = {http://mnras.oxfordjournals.org/content/early/2014/01/09/mnras.stt2383.abstract},
}

@Article{broucke1971,
