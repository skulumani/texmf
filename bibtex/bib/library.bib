@Article{durrant-whyte2006,
  author     = {H. Durrant-Whyte and T. Bailey},
  title      = {Simultaneous localization and mapping: part I},
  journal    = {IEEE Robotics Automation Magazine},
  year       = {2006},
  volume     = {13},
  number     = {2},
  pages      = {99--110},
  month      = jun,
  issn       = {1070-9932},
  abstract   = {This paper describes the simultaneous localization and mapping (SLAM) problem and the essential methods for solving the SLAM problem and summarizes key implementations and demonstrations of the method. While there are still many practical issues to overcome, especially in more complex outdoor environments, the general SLAM method is now a well understood and established part of robotics. Another part of the tutorial summarized more recent works in addressing some of the remaining issues in SLAM, including computation, feature representation, and data association},
  comment    = {* Build a consistent map of an unknown enviornment while determining own location within the map
* At a theoretical and conceptual level, SLAM can now be considered a solved problem
* Key early contribution was the recognition that there is a high degree of correlation between estimates of the location of different landmarks in the map, and that the correlation between landmarks would grow with more observations.
* Another early contribution was the fact that a consitent full solution would require a joint state composed of the vehicle pose and every landmark position to be updated with each observation. This would require a large state vector that required computations which scale as the square of the number of landmarks
* Conceptual breakthrough was the formulating the problem as a combined mapping and localization estimation problem would lead to a convergent solution. More measurements will lead to a converged map and localization estimate
* The ideal goal is to make a recursive solution for the SLAM problem.
* Using a Bayseian probabilistic framework allows for a two-step predictor/corrector setup to estimate robot pose and map landmarks conditioned on inputs, measurements, and preivous states
* Error between estimated and true landmark locations is common between landmarks and is in fact due to a single source; errors in the knowledge of the robot
* Relative locations between two landmarks are known with high accuracy
* Correlation between landmark estimates increases monotonically with more observations and that knowledge of relative landmark location always improves and never diverges regardless of robot motion
* The measurements can be considered as nearly independent measurements of the relative location between landmarks
* Most common approach to solve probablistic SLAM - State space approach with additive gaussian noise using EKF-SLAM or represent more general particle filter approach for general distributions using the FastSLAM
* There is a large computational effort as each update step requires that all landmarks and the joint covariance matrix must be updated each time a measurement is made
* Another important consideration is that correct data association will cause SLAM to completely fail
* Measurment/Landmark association becomes even more difficult when landmarks look different from different views
* The key improvement with FastSLAM is the map is represented as a set of independent Gaussians with linear complexity rather than a joint map covariance with quadratic complexity
* Since we're dealing with particle filters we must resample to remove low weighted particles. However SIS sampling is only good for systems that exponentially forget their past, future states should become increasingly independent of previous states},
  doi        = {10.1109/MRA.2006.1638022},
  file       = {:durrant-whyte2006.pdf:PDF},
  groups     = {SLAM},
  keywords   = {mobile robots;path planning;SLAM problem;mobile robots;simultaneous localization and mapping problem;Artificial intelligence;Bayesian methods;Buildings;History;Mobile robots;Navigation;Particle filters;Robotics and automation;Simultaneous localization and mapping;Vehicles, read, rank5},
  owner      = {shankar},
  timestamp  = {2018.05.11 15:05},
}

@Article{bailey2006,
  author     = {T. Bailey and H. Durrant-Whyte},
  title      = {Simultaneous localization and mapping (SLAM): part II},
  journal    = {IEEE Robotics Automation Magazine},
  year       = {2006},
  volume     = {13},
  number     = {3},
  pages      = {108--117},
  month      = sep,
  issn       = {1070-9932},
  abstract   = {This paper discusses the recursive Bayesian formulation of the simultaneous localization and mapping (SLAM) problem in which probability distributions or estimates of absolute or relative locations of landmarks and vehicle pose are obtained. The paper focuses on three key areas: computational complexity; data association; and environment representation},
  comment    = {* Some key hurdles that need to be addressed for SLAM: lots of computations which grows as more landmarks are added, correct association of measurements with landmarks, storing richer data about the landmarks (appearance/shape etc)
* In the state-based SLAM formulation the process model only updates the robot state, and the observation state only updates the landmarks
* There are some methods to reduce complexity in the EKF-SLAM approach:
  * Re-formulate the time-update using state-augmentation - assume the motion/dynamics only operate on robot state adn not the map
    * Seperate state adn map in EKF allows for new landmarks to be reduced to linear calculations
  * Observeration update can be partioned - only update a local map which is infreqently updated to the global map
    * sensor rate updates are confined to a smaller local region
    * Global pose is possible through vector addition of local maps
    * Global map is updated at a much lower freqency
  * Using information matrix form - creates sparse matrices which is faster
    * Many off-diagonal elements of the information matrix are zero
    * Efficient update procedures are possible using the sparse matrices
  * Sub-mapping allows for updates in a local map instead of the global map
    * Nearby landmarks are defined in a local coordinate frame
* Data association using batch gating. Also can use a multiple hypothesis approach to propogate multiple associations at once
  * Batch gating is usually sufficient to achieve reliable association},
  doi        = {10.1109/MRA.2006.1678144},
  file       = {:bailey2006.pdf:PDF},
  groups     = {SLAM},
  keywords   = {Bayes methods;computational complexity;mobile robots;path planning;statistical distributions;computational complexity;data association;environment representation;mobile robot;probability distributions;recursive Bayesian formulation;simultaneous localization and mapping;vehicle pose;Bayesian methods;Computational complexity;Computational efficiency;Delay estimation;Mobile robots;Robotics and automation;Robustness;Simultaneous localization and mapping;Uncertainty;Vehicles, read, rank5},
  owner      = {shankar},
  timestamp  = {2018.05.11 15:05},
}

