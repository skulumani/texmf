  issn          = {0743-1619},
  keywords      = {Lie groups;attitude control;discrete time systems;nonlinear control systems;predictive control;space vehicles;stability;vehicle dynamics;Lie group variational integrator;SO(3);constrained spacecraft attitude control;global rest-to-rest reorientation maneuvers;inclusion zone constraints;nominal controller;nonlinear discrete-time model;nonlinear model predictive control;nonlinear reference governor;spacecraft dynamics;stability properties;torque constraints;Aerodynamics;Attitude control;Computational modeling;Predictive control;Predictive models;Space vehicles;Symmetric matrices;Predictive control for nonlinear systems;Spacecraft control},
