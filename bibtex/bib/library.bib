@InProceedings{tanimoto2013,
  author     = {A. Tanimoto and N. Takeishi and T. Yairi and Y. Tsuda and F. Terui and N. Ogawa and Y. Mimasu},
  title      = {Fast estimation of asteroid shape and motion for spacecraft navigation},
  booktitle  = {2013 IEEE International Conference on Robotics and Biomimetics (ROBIO)},
  year       = {2013},
  pages      = {1550--1555},
  month      = dec,
  abstract   = {In this paper, we consider fast simultaneous estimation problem of the geometric shape of the asteroid and the relative motion of the spacecraft. In asteroid exploration missions, the information of asteroid shape and motion is needed to find suitable landing sites and navigate the spacecraft safely. In the previous HAYABUSA mission, large part of the estimation was performed manually by ground operators. We propose an efficient automatic estimation method using the image feature matching and matrix decomposition based fast 3D reconstruction techniques. Preliminary experiment results are also shown.},
  comment    = {* Uses a vision/image sensor to take pictures of an asteroid to both estimate relative motion and the shape of the asteroid
* It seems to use a version of the matrix decompisition which can simultaneously estimate motion of features and the relative motion of spacecraft
* The paper makes many simplifying assumptions which greatly restricts the usefulness:
  * The spacecraft orbits in a fixed plane relative to the asteroid
  * The asteroid is rotating without nutation - fixed axis spin
  * The spacecraft orbits in a plane orthogonal to the asteroid rotation - equatorial orbit
  * Spherical asteroid
  * Spacecraft is far from asteroid - can see an entire hemisphere
  * Uses a small angle approximation and linear motion model for landmarks between images},
  doi        = {10.1109/ROBIO.2013.6739687},
  file       = {:tanimoto2013.pdf:PDF},
  groups     = {SLAM, Shape Reconstruction},
  keywords   = {aerospace computing;aerospace safety;aircraft navigation;asteroids;entry, descent and landing (spacecraft);image matching;image reconstruction;matrix decomposition;motion estimation;space vehicles;HAYABUSA mission;asteroid exploration mission;asteroid shape estimation;automatic estimation method;fast 3D reconstruction technique;geometric shape estimation;image feature matching;landing site;matrix decomposition;relative motion estimation;spacecraft navigation;Cameras;Estimation;Image reconstruction;Matrix decomposition;Shape;Space vehicles;Three-dimensional displays, read},
  owner      = {shankar},
  timestamp  = {2018.05.11 15:05},
}

@Article{aulinas2008,
  author     = {Aulinas, Josep and Petillot, Yvan R and Salvi, Joaquim and Llad{\'o}, Xavier},
  title      = {The SLAM problem: a survey.},
  journal    = {CCIA},
  year       = {2008},
  volume     = {184},
  number     = {1},
  pages      = {363--371},
  comment    = {* Short overview of SLAM 
* There are several main probablisitic  approaches to teh SLAM problem
  * Kalman filter - assumption of additive gaussian error in state and measurement models. Information filter version is supposed to be better
    * Eventually there will be many landmarks and matrix inversion becomes expensive
  * Particle filter - able to handle non Gaussian distributions. Only been applied to the localization and not really the mapping portion yet
    * FASTSLAM is a good version which seperates localization and landmark estimation into seperate problems. This particle approach allows for many possible data associations in comparision to KF SLAM which is stuck with only a single data association
  * Expectation maximization method - two step process for getting an ideal map rather than the localization
    * Able to handle the data association problem very well. 
    * Iteratively  localizes the robot relative to a map by generating various hypotheses of robot location which are then used in a second step of translation to map features
* Key problem is associating measurements with specific landmarks in teh map. Getting this wrong can break SLAM
  * Better to use more features, 3D features rather than only 2D information
* Paper has a simple table listing the pros vs cons of the various SLAM approaches},
  file       = {:aulinas2008.pdf:PDF},
  groups     = {SLAM},
  keywords   = {read},
  owner      = {shankar},
  timestamp  = {2018.05.11 15:05},
}

