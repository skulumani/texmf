@Article{sonnenburg2010,
  author    = {Arne Sonnenburg and Marcel Tkocz and Klaus Janschek},
  title     = {EKF-SLAM based Approach for Spacecraft Rendezvous Navigation with Unknown Target Spacecraft},
  journal   = {IFAC Proceedings Volumes},
